{
  "name": "@spryrocks/capacitor-ionic-core-plugin",
  "version": "2.2.6",
  "description": "Ionic plugin core capacitor",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "license": "MIT",
  "author": "",
  "repository": {
    "url": "https://github.com/SpryRocks/ionic-plugin-core-capacitor"
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "SpryrocksCapacitorIonicCorePlugin.podspec",
    "ios/"
  ],
  "scripts": {
    "compile:native": "npm run build:android",
    "build:android": "(cd android && ./gradlew build)",
    "lint": "eslint './**/*.{ts,tsx}' --cache",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "build:develop": "tsc --watch",
    "prepublishOnly": "npm run build",
    "compile": "tsc --noEmit"
  },
  "dependencies": {
    "@spryrocks/logger-plugin": "^0.1.1"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.7"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@spryrocks/eslint-config": "^0.2.8-alpha.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "capacitor": {
    "android": {
      "src": "android"
    },
    "ios": {
      "src": "ios"
    }
  }
}
